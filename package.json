{
  "name": "@ctrl/utorrent",
  "version": "0.0.0-placeholder",
  "description": "TypeScript api wrapper for utorrent using got",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/utorrent",
  "homepage": "https://utorrent.vercel.app",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "utorrent",
    "typescript"
  ],
  "scripts": {
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "prepare": "npm run build",
    "build": "tsc",
    "build:docs": "typedoc",
    "test": "ava",
    "test:watch": "ava --watch",
    "test:ci": "c8 --reporter=text --reporter=lcov ava"
  },
  "dependencies": {
    "@ctrl/shared-torrent": "^4.0.0",
    "@ctrl/torrent-file": "^2.0.1",
    "formdata-node": "^4.3.1",
    "form-data-encoder": "^1.7.1",
    "got": "^12.0.0",
    "tough-cookie": "^4.0.0",
    "@ctrl/url-join": "^2.0.0"
  },
  "devDependencies": {
    "@ctrl/eslint-config": "3.2.0",
    "@types/node": "17.0.1",
    "@types/tough-cookie": "4.0.1",
    "@sindresorhus/tsconfig": "2.0.0",
    "ava": "4.0.0-rc.1",
    "c8": "7.10.0",
    "tsm": "2.2.1",
    "typedoc": "0.22.10",
    "typescript": "4.5.4"
  },
  "ava": {
    "files": [
      "test/**/*.spec.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=tsm"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=10.19.0"
  }
}
