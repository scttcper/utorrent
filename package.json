{
  "name": "@ctrl/utorrent",
  "version": "0.0.0-placeholder",
  "description": "TypeScript api wrapper for utorrent using got",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/utorrent",
  "homepage": "https://utorrent.vercel.app",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "utorrent",
    "typescript"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --fix --ext .js,.ts .",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json",
    "build:docs": "typedoc",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:ci": "npm run test -- --ci --reporters=default --reporters=jest-junit --coverage",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@ctrl/shared-torrent": "^3.0.5",
    "@ctrl/torrent-file": "^1.3.3",
    "form-data": "^4.0.0",
    "got": "^11.8.2",
    "tough-cookie": "^4.0.0",
    "@ctrl/url-join": "^1.0.4"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@ctrl/eslint-config": "3.1.1",
    "@jest/globals": "27.3.1",
    "@types/node": "16.11.6",
    "@types/tough-cookie": "4.0.1",
    "jest": "27.3.1",
    "jest-junit": "13.0.0",
    "typedoc": "0.22.8",
    "typescript": "4.4.4"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageProvider": "v8"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=10.19.0"
  }
}
