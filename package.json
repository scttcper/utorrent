{
  "name": "@ctrl/utorrent",
  "version": "0.0.0-placeholder",
  "description": "TypeScript api wrapper for utorrent using got",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/utorrent",
  "homepage": "https://utorrent.vercel.app",
  "type": "module",
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "sideEffects": false,
  "keywords": [
    "utorrent",
    "typescript"
  ],
  "scripts": {
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "prepare": "npm run build",
    "build": "tsc",
    "build:docs": "typedoc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage --reporter=default --reporter=junit --outputFile=./junit.xml"
  },
  "dependencies": {
    "@ctrl/magnet-link": "^3.1.1",
    "@ctrl/shared-torrent": "^4.2.0",
    "@ctrl/torrent-file": "^2.0.2",
    "@ctrl/url-join": "^2.0.2",
    "formdata-node": "^5.0.0",
    "got": "^12.3.1",
    "tough-cookie": "^4.1.2"
  },
  "devDependencies": {
    "@ctrl/eslint-config": "3.4.10",
    "@sindresorhus/tsconfig": "3.0.1",
    "@types/node": "18.7.14",
    "@types/tough-cookie": "4.0.2",
    "@vitest/coverage-c8": "0.22.1",
    "c8": "7.12.0",
    "typedoc": "0.23.12",
    "typescript": "4.8.2",
    "vitest": "0.22.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "engines": {
    "node": ">=14.16"
  }
}
